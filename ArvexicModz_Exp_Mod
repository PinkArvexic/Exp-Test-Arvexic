_G.Farm = true -- change to true or false
        while _G.Farm and wait() do
    local args = { 

        [3] = workspace.Eggs.Egg9
    } 

    game:GetService("ReplicatedStorage").EggEvent:FireServer(unpack(args)) 
    local args = { 

        [5] = workspace.Treasures.Treasure1

    } 

    game:GetService("ReplicatedStorage").TreasureEvent:FireServer(unpack(args)) 
    local args = { 

        [5] = workspace.Treasures.Treasure2

    } 

    game:GetService("ReplicatedStorage").TreasureEvent:FireServer(unpack(args)) 
    local args = { 

        [5] = workspace.Treasures.Treasure3

    } 

    game:GetService("ReplicatedStorage").TreasureEvent:FireServer(unpack(args)) 
end
